<ADE_HTML_2_1_tools_Config>
	<!-- NOTE: this config is only here for old content that has been localized and not yet updated with the Source Access changes. -->
	<project>
		<output>..\output\developer_help\%SR_DOC_LANG%\preview</output>
		<AKN>
			<component_id>Stingray-Developer-Help</component_id>
			<product>Stingray</product>
			<release>NA</release>
			<language>%SR_DOC_LANG_AKN%</language>
			<book>Developer</book>
			<local_output>..\output\developer_help\%SR_DOC_LANG%\AKN</local_output>
			<post_to_AKN>false</post_to_AKN>
			<post_to_AKN_beta>false</post_to_AKN_beta>
		</AKN>
		<language>%SR_DOC_LANG%</language>
		<project_title>Stingray Developer Help</project_title>
		<project_title lang="ko">Stingray 개발자 도움말</project_title>
		<project_title lang="ja">Stingray 開発者ヘルプ</project_title>
		<export_for_localization>%SR_DOC_EXPORT_DIR%</export_for_localization>
		<import_from_localization>%SR_DOC_IMPORT_DIR%</import_from_localization>
	</project>

	<global>
		<syntax_coloring include="\developer\*" default_lang="lang-lua">true</syntax_coloring>
		<!--search_filter id="prog" include_expression="files/.*htm">
			<label lang="en">Search Programming Guide?</label>
		</search_filter>
		<search_filter id="cpp_ref" include_expression="cpp_ref/((?!8h_source).)*\.html">
			<label lang="en">Search C++ API Reference?</label>
		</search_filter>
		<search_filter id="headers" include_expression="cpp_ref/.*8h_source\.html">
			<label lang="en">Search C++ header files?</label>
		</search_filter-->
		<contextId include="*\developer\introduction.html" id="DEVELOPER_HOME"></contextId>
		<!-- add_MT include="\developer\*" fromlanguage="en">true</add_MT -->
		<insert_family_links include="\developer\*"></insert_family_links>
	</global>

	<merge id="images">
		<source>..\source\resources\*.png</source>
		<merge_toc>false</merge_toc>
	</merge>

	<merge id="developer">
		<source convert_from="markdown">..\source\developer\*.md</source>
		<source>..\source\developer\*.png</source>
		<source>..\source\developer\*.jp?g</source>
		<source>..\source\developer\*.gif</source>
		<source>..\source\developer\*.toc</source>
		<search_prefix></search_prefix>
		<merge_toc_config>
			<source type="directory"></source>
			<promote_first_topic>false</promote_first_topic>
		</merge_toc_config>
	</merge>

</ADE_HTML_2_1_tools_Config>